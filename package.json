{
  "name": "wsu-marketshare",
  "version": "1.0.0",
  "description": "A digital MarketPlace exclusively for WSU students",
  "main": "index.js",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WSU-4110/WSU-MarketShare.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/WSU-4110/WSU-MarketShare/issues"
  },
  "homepage": "https://github.com/WSU-4110/WSU-MarketShare#readme",
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/runtime": "^7.27.0",
    "@testing-library/jest-dom": "^6.6.3",
    "babel-jest": "^29.7.0",
    "firebase": "^11.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx)$": [
        "babel-jest",
        {
          "rootMode": "upward"
        }
      ]
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@firebase|firebase)/)"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "frontend"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "^@/(.*)$": "<rootDir>/frontend/$1",
      "^firebase/(.*)$": "<rootDir>/node_modules/firebase/$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  }
}
